			<Mass>220</Mass>
			<MoveSpeed>13.5</MoveSpeed>
			<CargoCapacity>225</CargoCapacity>
			<RepairRate>0.8</RepairRate>
			<AccelerationRate>0.115</AccelerationRate>
		</vehicleStats>
		
		<soundSustainersOnEvent>
